<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net7.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<Configurations>Debug with args;Debug;Release</Configurations>
	</PropertyGroup>
	<PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug with args|AnyCPU' ">
		<IntermediateOutputPath>obj\Debug with args</IntermediateOutputPath>
		<DebugType>
		</DebugType>
		<Optimize>false</Optimize>
		<OutputPath>bin\Debug with args</OutputPath>
		<DefineConstants>
		</DefineConstants>
		<WarningLevel>4</WarningLevel>
		<NoWarn>
		</NoWarn>
		<NoStdLib>false</NoStdLib>
		<Nullable>
		</Nullable>
		<StartArguments>file.txt</StartArguments>
	</PropertyGroup>
</Project>